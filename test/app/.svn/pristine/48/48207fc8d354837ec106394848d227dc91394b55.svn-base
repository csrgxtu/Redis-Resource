/*   left    */
H1 {
	font-size: 15px;
	margin: 0px;
	width: 182px;
	cursor: pointer;
	height: 30px;
	line-height: 20px;	
}


h1 p{
	display: block;
	width: 182px;
	color: #004000;
	height: 30px;
	text-decoration: none;
	background: url(/images/user/menu_bgs.gif) no-repeat;
	line-height: 30px;
	text-align: center;
	margin: 0px;
	padding: 0px;

}


.MM {
	width: 182px;
	margin: 0px;
	padding: 0px;
	left: 0px;
	top: 0px;
	right: 0px;
	bottom: 0px;
	clip: rect(0px,0px,0px,0px);
}


.MM ul {
	list-style-type: none;
	margin: 0px;
	padding: 0px;
	display: block;
}

.MM li {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 14px;
	line-height: 26px;
	color: #333333;
	list-style-type: none;
	display: block;
	text-decoration: none;
	height: 26px;
	width: 182px;
	padding-left: 0px;
}


.MM span{
	font-family: Arial, Helvetica, sans-serif;
	font-size: 14px;
	line-height: 26px;
	color: #333333;
	background: url(/images/user/menu_bg1.gif) no-repeat;
	height: 26px;
	width: 182px;
	display: block;
	text-align: center;
	margin: 0px;
	padding: 0px;
	overflow: hidden;
	text-decoration: none;
}

.MM span:hover {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 14px;
	line-height: 26px;
	font-weight: bold;
	color: #006600;
	background-image: url(/images/user/menu_bg2.gif);
	background-repeat: no-repeat;
	text-align: center;
	display: block;
	margin: 0px;
	padding: 0px;
	height: 26px;
	width: 182px;
	text-decoration: none;
	cursor:pointer;
}
/*      right       */

.titlebt {
	font-size: 12px;
	line-height: 26px;
	color: #353C44;
	display: block;
	text-indent: 5px;
	background: url(/images/user/content-bg.gif) repeat-x;
}

#navigation {
	margin:0px;
	padding:0px;
	width:147px;
}

#navigation{
	list-style:none;
}

#gps{
	width:100%;
	height:100%;
	border:1px solid #D5DAFF;
}

#gpsMap{
	width:100%;
	height:90%;
}






