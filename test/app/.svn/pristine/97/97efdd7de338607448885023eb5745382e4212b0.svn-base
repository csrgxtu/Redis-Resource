/* 弹层css */
.black_overlay{
display:none;
position: absolute;
top: 0%;
left: 0%;
width: 100%;
height: 100%;
background-color: black;
z-index:1001;
-moz-opacity: 0.5;
opacity:0.2;
filter: alpha(opacity=20);
}
.white_content {
display:none;
position: absolute;
top: 15%;
left: 15%;
width: 930px;
